{
	"bsonType": "object",
	"required": [],
	"permission": {
		"read": false,
		"create": false,
		"update": false,
		"delete": false
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"create_date": {
			"bsonType": "timestamp",
			"description": "创建时间",
			"forceDefaultValue": {
				"$env": "now"
			}
		},
		"title": {
			"description": "公告标题",
			"bsonType": "string"
		},
		"content": {
			"description": "公告内容",
			"bsonType": "string"
		},
		"start_date": {
			"bsonType": "timestamp",
			"description": "开始时间"
		},
		"end_date": {
			"bsonType": "timestamp",
			"description": "结束时间"
		}
	}
}