// 文档教程: https://uniapp.dcloud.net.cn/uniCloud/schema
{
	"bsonType": "object",
	"required": [],
	"permission": {
		"read": false,
		"create": false,
		"update": false,
		"delete": false
	},
	"properties": {
		"_id": {
			"description": "ID系统自动生成"
		},
		"content": {
			"bsonType": "string",
			"description": "请输入内容",
			"title": "内容"
		},
		"create_time": {
			"bsonType": "timestamp",
			"description": "请输入创建时间",
			"forceDefaultValue": {
				"$env": "now"
			}
		},
		"create_uid": {
			"bsonType": "string",
			"description": "请输入创建人id",
			"forceDefaultValue": {
				"$env": "uid"
			}
		},
		"is_delete": {
			"bsonType": "bool",
			"defaultValue": "请输入是否删除",
			"title": "删除"
		},
		"state": {
			"bsonType": "string",
			"defaultValue": "请输入状态",
			"title": "状态"
		},
		"type": {
			"bsonType": "string",
			"defaultValue": "请输入类型",
			"title": "类型"
		},
		"url": {
			"bsonType": "string",
			"defaultValue": "请输入地址",
			"title": "地址"
		},
		"version": {
			"bsonType": "string",
			"defaultValue": "请输入版本号",
			"title": "版本号"
		}
	}
}

